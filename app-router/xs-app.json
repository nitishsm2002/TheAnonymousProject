{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/resources/(.*)$",
      "target": "/resources/$1",
      "service": "html5-apps-repo-rt"
    },
    {
      "source": "^/odata/(.*)$",
      "target": "/odata/$1",
      "destination": "anonymous-cap-backend",
      "csrfProtection": true
    }
  ]
}

